Shielded 32 group problem

  start problem
    execution = yes
    lambda = 0
    type=fsrc ;
    sweep=precomp
    page_refl=save
    innerconv=1E-12 ; outerconv=1E-8
    maxinner = 4000 ; maxouter = 5000
  end problem

  start inout
    mesh_file     = ./noshield.thrm
    xs_file       = ./noshield_32group.xs
    source_file   = ./noshield_32group.src
    flux_file     = noshieldout.flux
    print_xs = no
  end inout

  start cross_sections
     ngroups=32 ; upscattering = yes ;
     multiplying = yes
  end cross_sections

  start quadrature
    qdtype=levelsym ; qdorder = 2
  end quadrature

  start regionmap
     1
     2
     3
     4
     5
     6
     7
  end regionmap

end files